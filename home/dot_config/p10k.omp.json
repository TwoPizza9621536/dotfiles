{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "template": "{{ .Icon }} "
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "180",
          "foreground_templates": [
            "{{ if .Root }}178{{ end }}"
          ],
          "template": "{{ if .Root }}<b>{{ end}}{{ .UserName }}@{{ .HostName }}{{ if .Root }}</b>{{ end }} "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "31",
          "template": " {{ .Path }} ",
          "properties": {
            "home_icon": "~",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "76",
          "template": "{{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "github_icon": ""
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "168",
          "template": "  {{ .Full }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "37",
          "template": "  {{ .Full }}"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "37",
          "template": " ﳑ {{ .Full }}"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "70",
          "template": "  {{ .Full }}"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "37",
          "template": "  {{ .Full }}"
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "134",
          "template": "  {{ .Full }}"
        },
        {
          "type": "flutter",
          "style": "plain",
          "foreground": "38",
          "template": "  {{ .Full }}"
        },
        {
          "type": "lua",
          "style": "plain",
          "foreground": "32",
          "template": "  {{ .Full }}"
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "32",
          "template": "  {{ .Full }}"
        },
        {
          "type": "perl",
          "style": "plain",
          "foreground": "67",
          "template": "  {{ .Full }}"
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "99",
          "template": "  {{ .Full }}"
        },
        {
          "type": "haskell",
          "style": "plain",
          "foreground": "172",
          "template": "  {{ .Full }}"
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "70",
          "template": "  {{ .Full }}"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{ if eq \"Charging\" .State.String }}70{{ end }}",
            "{{ if eq \"Full\" .State.String }}70{{ end }}"
          ],
          "template": " {{ .Icon }}{{ .Percentage }}%",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": " ",
            "charged_icon": " "
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "66",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "101",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "threshold": 3000,
            "style": "round"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "76",
          "foreground_templates": [
            "{{ if ne .Code 0 }}196{{ end }}"
          ],
          "template": "❯ "
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "76",
    "foreground_templates": [
      "{{ if ne .Code 0 }}196{{ end }}"
    ],
    "template": "❯ "
  }
}
