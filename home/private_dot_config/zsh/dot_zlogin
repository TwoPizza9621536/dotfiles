## Aliases
alias cat=bat
alias cloc=scc
alias diff=batdiff
alias find=fd
alias fzf="fzf --preview 'bat --style=numbers --color=always --line-range :500 {}'"
alias grep=batgrep
alias man=batman
alias neofetch=hyfetch
alias svn="svn --config-dir $XDG_CONFIG_HOME/subversion"
alias tb="toolbox enter"

eval $(thefuck --alias)

neofetch
